{{define "title"}}Your Account{{end}}

{{define "main"}}
    <h2>Your Account</h2>

    <table>
        <tr>
            <th>Name</th>
            <td>{{.Form.Name}}</td>
        </tr>
        <tr>
            <th>Email</th>
            <td>{{.Form.Email}}</td>
        </tr>
        <tr>
            <th>Joined</th>
            <td> {{humanDate .Form.Created}} </td>
        </tr>
    </table>
    
{{end}}
